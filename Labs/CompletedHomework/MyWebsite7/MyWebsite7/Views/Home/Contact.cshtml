@model Dictionary<string, string>

@{
    ViewData["Title"] = "Contact me";
}

@section header {
    <h1>Contact me</h1>
}

<table class="table table-bordered table-striped">
    @foreach (KeyValuePair<string, string> pair in Model)
    {
        <tr>
            <td>@pair.Key</td>
            <td>@pair.Value</td>
        </tr>
    }
</table>